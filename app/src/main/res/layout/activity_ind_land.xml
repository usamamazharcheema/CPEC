<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f2f2">

    <RelativeLayout
        android:layout_below="@+id/theme"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_45sdp"
        android:id="@+id/topheader"
        android:background="@drawable/savabg"

        >
        <ImageView
            android:layout_centerVertical="true"
            android:id="@+id/isearcopenhback"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_25sdp"
            android:src="@drawable/arrowback"
            />
        <TextView
            android:id="@+id/titleplotopserachfiltermain"
            android:textColor="#fff"
            android:textSize="@dimen/_17sdp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="INDUSTRIAL/WAREHOUSE LAND"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/topheader">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center_horizontal">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Darbela Janoobi"
                android:layout_marginTop="@dimen/_35sdp"
                android:textColor="#298f91"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"
                android:textSize="@dimen/_15sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahd"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Surbandar"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"

                android:textColor="#298f91"
                android:textSize="@dimen/_16sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahsu"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Shankanidar"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"

                android:textColor="#298f91"
                android:textSize="@dimen/_16sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahsh"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Khappar"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"

                android:textColor="#298f91"
                android:textSize="@dimen/_16sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahkh"
                />

            <Button
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_60sdp"
            android:text="Mozah Nailent"
            android:layout_marginTop="@dimen/_2sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_14sdp"

            android:textColor="#298f91"
            android:textSize="@dimen/_16sdp"
            android:background="@drawable/allbg"
            android:id="@+id/mozahnai"
            />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Ziarat Machi Sharqi"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"

                android:textColor="#298f91"
                android:textSize="@dimen/_16sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahzia"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Jorkan"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"

                android:textColor="#298f91"
                android:textSize="@dimen/_16sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahjo"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Chibrikani"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"

                android:textColor="#298f91"
                android:textSize="@dimen/_16sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahchi"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_60sdp"
                android:text="Mozah Darbela Shumali"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_14sdp"

                android:textColor="#298f91"
                android:textSize="@dimen/_16sdp"
                android:background="@drawable/allbg"
                android:id="@+id/mozahdar"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:id="@+id/con6"

                android:background="@drawable/cu"/>
        </LinearLayout>

    </ScrollView>

    <RelativeLayout
        android:id="@+id/theme"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_56sdp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="22dp"
        android:layout_alignParentEnd="true">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8sdp"
            android:src="@drawable/explore_top_view"
            />
    </RelativeLayout>

</RelativeLayout>




