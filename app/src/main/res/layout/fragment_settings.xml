<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
<LinearLayout
    android:layout_width="wrap_content"
android:orientation="vertical"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@+id/first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <LinearLayout
        android:paddingLeft="@dimen/_20sdp"
        android:paddingRight="@dimen/_20sdp"
        android:id="@+id/layoutfav"
        android:layout_width="wrap_content"
        android:orientation="horizontal"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/favoriteicon"
            />

        <TextView
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:id="@+id/favourites"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Favourite"
            />

    </LinearLayout>
        <View
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#5e9fa6"
            android:layout_below="@+id/layoutfav"
            >


        </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/_7sdp"
        android:id="@+id/second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingLeft="@dimen/_20sdp"
            android:paddingRight="@dimen/_20sdp"
            android:id="@+id/layoutnewsearch"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/newsearchicon"
                />

            <TextView
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/newsearch"
                />

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#5e9fa6"
            android:layout_below="@+id/layoutnewsearch"
            >


        </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/_7sdp"
        android:id="@+id/third"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingLeft="@dimen/_20sdp"
            android:paddingRight="@dimen/_20sdp"
            android:id="@+id/layoutadproperty"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/addpropertyicon"
                />

            <TextView
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/addprpoerty"
                />

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#5e9fa6"
            android:layout_below="@+id/layoutadproperty"
            >


        </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/_7sdp"
        android:id="@+id/forth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingLeft="@dimen/_20sdp"
            android:paddingRight="@dimen/_20sdp"
            android:id="@+id/layoutMyproperties"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/mypropertyicon"
                android:id="@+id/imageView2" />

            <TextView
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/myyprpoerty"
                />

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#5e9fa6"
            android:layout_below="@+id/layoutMyproperties"
            >


        </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/_7sdp"
        android:id="@+id/fifth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingLeft="@dimen/_20sdp"
            android:paddingRight="@dimen/_20sdp"
            android:id="@+id/layoutcontact"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/contactusicon"
                />

            <TextView
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/contactus"
                />

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#5e9fa6"
            android:layout_below="@+id/layoutcontact"
            >


        </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/_7sdp"
        android:id="@+id/sixth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingLeft="@dimen/_20sdp"
            android:paddingRight="@dimen/_20sdp"
            android:id="@+id/layoutabout"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/aboutusicon"
                android:id="@+id/imageView" />

            <TextView
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/aboutus"
                />

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#5e9fa6"
            android:layout_below="@+id/layoutabout"
            >


        </View>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/_7sdp"
        android:id="@+id/seventh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout
            android:paddingLeft="@dimen/_20sdp"
            android:paddingRight="@dimen/_20sdp"
            android:id="@+id/layoutSingedout"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/logouticon"
                />

            <TextView
                android:textSize="@dimen/_15sdp"
                android:textStyle="bold"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/signout"
                android:id="@+id/textView2" />

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/_7sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="#5e9fa6"
            android:layout_below="@+id/layoutSingedout"
            >


        </View>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">
        <TextView
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/poweredby"
            android:textColor="#000000"
            android:textSize="@dimen/_12sdp"
            android:id="@+id/powered"
            android:textStyle="bold"
            />
        <ImageView
            android:layout_centerHorizontal="true"
            android:layout_width="@dimen/_43sdp"
            android:layout_height="@dimen/_63sdp"
            android:id="@+id/biz"
            android:src="@drawable/bizlogo"
            android:layout_marginTop="@dimen/_28sdp"/>
        <TextView
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/biz"
            android:textSize="@dimen/_9sdp"
            android:id="@+id/venture"
            android:textStyle="bold"
            android:layout_below="@+id/biz"

            />

    </RelativeLayout>
</LinearLayout>
    </ScrollView>
</RelativeLayout>